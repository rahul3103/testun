    "husky": ">=3.1.0",
    "lint-staged": ">=9.5.0",
    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1"
