    "next": "9.1.4",
    "next-redux-wrapper": "^4.0.1",
    "normalizr": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "styled-components": "^5.0.0-rc.2",
    "styled-normalize": "^8.0.6",
    "swr": "^0.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
